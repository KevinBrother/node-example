{
  "name": "koa-drive",
  "version": "0.2.3",
  "description": "Koa drive By TypeScript",
  "author": "liyuanquan",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon app.ts",
    "serve": "ts-node -r tsconfig-paths/register app.ts",
    "build": "ttsc",
    "start": "node ./dist/app.js",
    "pm2:start": "pm2 start pm2.json --env env-dev",
    "pm2:stop": "pm2 stop all",
    "pm2:log": "pm2 logs",
    "test:unit": "jest --config jest.config.js",
    "upgrade": "yarn upgrade",
    "interactive": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "koa": "^2.13.0",
    "koa-body": "^4.2.0",
    "koa-router": "^8.0.8",
    "koa2-cors": "^2.0.6",
    "koa2-swagger-ui": "^2.16.0",
    "log4js": "^6.3.0",
    "lowdb": "^1.0.0",
    "mongoose": "^5.9.27",
    "nodemon": "^2.0.4",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "pm2": "^4.4.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.5",
    "ts-node": "^8.10.2",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/core-js": "^2.5.3",
    "@types/jest": "^25.1.4",
    "@types/koa": "^2.11.3",
    "@types/koa-router": "^7.4.1",
    "@types/koa2-cors": "^2.0.1",
    "@types/lowdb": "^1.0.9",
    "@types/mongoose": "^5.7.34",
    "@types/uuid": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-jest": "^25.2.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "jest": "^25.2.0",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.10",
    "typescript": "^3.9.7"
  }
